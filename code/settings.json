{
  "debug.console.fontSize": 12,
  "editor.fontSize": 12,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
      80
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "off",
  "emmet.triggerExpansionOnTab": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/__pycache__": true,
    "**/._.*": true,
    "**/.DS_Store": true,
    "**/.attic": true,
    "**/.cvs": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.iml": true,
    "**/.ipr": true,
    "**/.iws": true,
    "**/.metadata": true,
    "**/.nb-gradle": true,
    "**/.recommenders": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.tmp": true,
    "**/*.7z": true,
    "**/*.gz": true,
    "**/*.pyc": true,
    "**/*.sqlite3": true,
    "**/*.tar": true,
    "**/*.zip": true,
    "**/nbbuild": true,
    "**/nbdist": true,
    "**/nbproject/private": true,
    "**/RemoteSystemsTempFiles": true,
    "**/Thumbs.db": true
  },
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/__pycache__/**": true,
    "**/.cvs/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.idea/**": true,
    "**/.iml": true,
    "**/.ipr": true,
    "**/.iws": true,
    "**/.metadata/**": true,
    "**/.nb-gradle/**": true,
    "**/.sass-cache/**": true,
    "**/.svn/**": true,
    "**/.tmp/**": true,
    "**/assets/libs/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true,
    "**/nbbuild/**": true,
    "**/nbdist/**": true,
    "**/nbproject/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "**/target/**": true,
    "**/temp/**": true,
    "**/plugins/**": true,
    "**/tmp/**": true
  },
  "files.associations": {
    "**/requirements{/**,*}.{txt,in}": "pip-requirements"
  },
  "git.enableSmartCommit": true,
  "search.exclude": {
    "**/__pycache__/**": true,
    "**/.cvs/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.idea/**": true,
    "**/.iml": true,
    "**/.ipr": true,
    "**/.iws": true,
    "**/.metadata/**": true,
    "**/.nb-gradle/**": true,
    "**/.sass-cache/**": true,
    "**/.svn/**": true,
    "**/.tmp/**": true,
    "**/dist/**": true,
    "**/nbbuild/**": true,
    "**/nbdist/**": true,
    "**/nbproject/**": true,
    "**/target/**": true,
    "**/temp/**": true,
    "**/tmp/**": true,
    "**/plugins/**": true,
    "**/venv/**": true
  },
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.profiles.linux": {
    "zsh -l": {
      "path": "/bin/zsh",
      "args": ["-l"]
    },
    "bash -l": {
      "path": "/bin/bash",
      "args": ["-l"]
    }
  },
  "terminal.integrated.profiles.osx": {
    "zsh -l": {
      "path": "/bin/zsh",
      "args": ["-l"]
    }
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell -NoProfile": {
      "source": "PowerShell",
      "args": ["-NoProfile"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell -NoProfile",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": false,
  "python.linting.pylintCategorySeverity.convention": "Hint",
  "python.linting.pylintCategorySeverity.warning": "Error",
  "python.linting.flake8Enabled": true,
  "python.formatting.blackArgs": ["--line-length", "79"],
  "[django-html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false,
    "editor.wordWrap": "on",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
      "editor.rulers": [
          72,
          79
      ]
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "prettier.singleQuote": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "plantuml.exportFormat": "png",
  "plantuml.diagramsRoot": "docs/diagrams/src/",
  "plantuml.exportOutDir": "docs/diagrams/imgs/",
  "plantuml.commandArgs": [
      "-Xmx1024m",
      "-DPLANTUML_LIMIT_SIZE=10240"
  ],
  "python.languageServer": "Pylance",
  "editor.emptySelectionClipboard": false,
  "rest-client.enableTelemetry": false,
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "workbench.editor.pinnedTabSizing": "shrink",
  "python.analysis.typeCheckingMode": "basic",
  "vsintellicode.features.python.deepLearning": "enabled",
  "rest-client.fontSize": 12,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "redhat.telemetry.enabled": true
}